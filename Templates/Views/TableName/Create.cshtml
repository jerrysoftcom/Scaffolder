@model <AppName>.Models.<TableName>

@{
    ViewBag.Title = "Title";
}

<h2>@ViewData["Title"]</h2>

<form asp-controller="<TableName>" asp-action="Create" method="post" class="form-horizontal" role="form">
    <div class="form-horizontal">
        <div asp-validation-summary="ValidationSummary.All" class="text-danger"></div>
        <div class="form-group">
            <label asp-for="<ColumnName>" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="<ColumnName>" class="form-control" />
                <span asp-validation-for="<ColumnName>" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
</form>